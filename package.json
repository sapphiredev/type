{
  "name": "@klasa/type",
  "version": "0.0.1",
  "description": "javascript type detection.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "ava",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "yarn coverage && nyc check-coverage --lines 90 --functions 90 --branches 90",
    "lint": "eslint --fix --ext ts src test"
  },
  "keywords": [],
  "author": "dirigeants",
  "license": "MIT",
  "engines": {
    "node": ">=10.1.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "ava": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "dirigeants/klasa-lint",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dirigeants/type"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  }
}
